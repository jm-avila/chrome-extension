{
  "manifest_version": 2,
  "name": "Search Engine Launcher",
  "description": "Quick launch",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
